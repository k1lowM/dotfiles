Host github github.com
  HostName github.com
  IdentityFile ~/.ssh/id_git_rsa

Host smib-devel
	HostName 192.168.179.21
	User ccds
	Port 2222
	UserKnownHostsFile /dev/null
	PreferredAuthentications publickey
	StrictHostKeyChecking no
	PasswordAuthentication no
	IdentityFile ~/.ssh/k1low
	IdentitiesOnly yes
	LogLevel FATAL

Host mms
	#HostName mktg.ccds.net
	HostName 10.1.1.80
	User ccds
	# IdentityFile ~/.ssh/uec/common.key
	Port 22
	TCPKeepAlive yes
	IdentitiesOnly yes

# CCDSサーバ
Host ccds
	HostName w1.ccds.net
	User ccds
	# IdentityFile ~/.ssh/uec/common.key
	Port 22
	TCPKeepAlive yes
	IdentitiesOnly yes

# 共通
Host *
	HostkeyAlgorithms +ssh-rsa
